</script>

<script id="window-popular-modal-tpl" type="text/template">
                <div class="b-order-repeat b-loading js-popular-products-modal">
                    <button class="b-order-repeat__close b-button b-button_close js-close" type="button" aria-label="Закрыть"></button>
                    <div class="js-popular-products-content"></div>
                </div>
            </script>

            <script id="window-popular-products-tpl" type="text/template">
                <h2 class="b-order-repeat__title">Часто покупаю</h2>
                <div class="b-order-repeat__block">
                    {% if (message): %}
                        <div class="b-order-repeat__descr">
                            {{= message }}
                        </div>
                        <div class="b-order-repeat__action">
                            <button class="b-order-repeat__btn b-button js-popular-products-add" type="button">
                                <span>Добавить все в корзину</span>
                            </button>
                        </div>
                    {% else: %}
                        <div class="b-order-repeat__descr">
                            К сожалению, в данный момент отсутствуют товары из вашего списка "Часто покупаю". Но у нас есть 15000+ других товаров в наличии
                        </div>
                    {% endif %}
                </div>

                <div class="b-order-repeat__products">
                    {% for (var i = 0; i < products.length; i++): %}
                        {% var product = products[i]; %}
                        {% var measure; %}
                        {% var quantity = 1; %}

                        {% if (product.productType.code === 'WEIGHT'): %}
                            {% if (product.showUnit === 'кг' || product.showUnit === 'кг.'): %}
                                {% measure = 'за 1 кг'; %}
                            {% else: %}
                                {% measure = 'за ' + product.showUnit; %}
                            {% endif %}
                        {% elseif (product.productType.code === 'PIECE_WEIGHT' || product.productType.code === 'PIECE'): %}
                            {% if (product.standardWeight): %}
                                {% measure = 'за 1 шт (' + product.standardWeight + ' г)'; %}
                            {% else: %}
                                {% measure = 'за 1 шт'; %}
                            {% endif %}
                        {% endif %}

                        {% if (product.inCartQuantity): %}
                            {% quantity = product.inCartQuantity; %}
                        {% elseif (product.productType.code === 'WEIGHT'): %}
                            {% if (product.weightStep): %}
                                {% quantity = product.weightStep; %}
                            {% else: %}
                                {% quantity = 100; %}
                            {% endif %}
                        {% endif %}

                        <article class="b-order-repeat__item b-product-line js-window-popular-product"
                            data-code="{{= product.code }}"
                            data-measure="{{= measure }}"
                            data-click-tracking-url="{{= product.clickTrackingURL }}"
                            data-pos="{{= i + 1 }}"
                            data-name="{{= product.name }}"
                            data-weighted="{{= product.productType.code === 'WEIGHT' }}"
                            data-facade-quantity="{{= quantity }}"
                            data-stock-quantity="{{= product.stock.stockLevel }}"
                            data-cart-quantity="{{= product.inCartQuantity }}"
                            data-unit-price="{{= product.unitPrice }}"
                            data-old-price="{{= product.oldPrice }}"
                            data-extra-old-price="{{= product.extraOldPrice }}"
                            data-total-price="{{= product.inCartTotalPrice ? product.inCartTotalPrice.value : product.unitPrice }}"
                            data-portal="{{= product.dataLayerEventAttrs.eventPortal }}"
                            data-category-id="{{= product.dataLayerEventAttrs.eventCategoryId }}"
                            data-category-name="{{= product.dataLayerEventAttrs.eventCategoryName }}"
                            data-brand="{{= product.dataLayerEventAttrs.brand }}"
                            data-weight-step="{{= product.weightStep }}"
                            data-variant="{{= product.dataLayerEventAttrs.variant }}"
                            data-is-alcohol-product="false"
                            >

                            <div class="b-product-line__col">
                                {% for (var j in product.images): %}
                                    {% if (product.images[j].format === 'thumbnail'): %}
                                        {% var image = product.images[j]; %}
                                        <a href="{{= app.config.context_path }}{{= product.url }}" title="{{= image.altText }}">
                                            <img class="b-product-line__img" src="{{= image.url }}" alt="{{= image.altText }}">
                                        </a>
                                        {% break; %}
                                    {% endif %}
                                {% endfor %}
                            </div>

                            <div class="b-product-line__col">
                                <a class="b-link" href="{{= app.config.context_path }}{{= product.url }}" title="{{= product.name }}">{{= product.name }}</a>

                                {% if (product.productLabel && product.productLabel.active): %}
                                    {% var colorCss = '#' + product.productLabel.colorCss; %}
                                    <span class="b-product-line__tag" style="background-color: {{= colorCss}}">
                                        {{= product.productLabel.text.substr(0, 42) }}
                                    </span>
                                {% endif %}
                            </div>

                            <div class="b-product-line__col b-product-line__count">
                                <span class="js-incart {{= product.inCartQuantity ? '' : 'm-hidden'}}">
                                    {% var dec_disabled = quantity <= (product.weighted ? 100 : 1); %}
                                    {% var inc_disabled = quantity >= product.stock.stockLevel; %}

                                    <button type="button" class="b-button b-button_green b-button_circle b-button_circle-minus js-count-dec" type="button"></button>

                                    {% if (product.productType.code === 'WEIGHT'): %}
                                        <div class="b-form__weight">
                                            <input name="number" class="b-form__number b-form__number_kg b-form__number_small js-count-input-kg" value="{{= app_format.weightKilos(quantity) }}" type="text" maxlength="3">
                                            <span class="b-form__number-text b-form__number-text_kg">
                                                кг.</span>
                                        </div>

                                        <div class="b-form__weight">
                                            <input name="number" class="b-form__number b-form__number_g b-form__number_small js-count-input-g" value="{{= app_format.weightGrams(quantity) }}" type="text" maxlength="3">
                                            <span class="b-form__number-text">
                                                г.</span>
                                        </div>
                                    {% else: %}
                                        <div class="b-form__weight">
                                            <input name="quantity" class="b-form__number b-form__number_g b-form__number_small js-count-input" value="{{= quantity }}" type="text" id="quantityfor{{= product.code }}" maxlength="3">
                                            <span class="b-form__number-text">шт.</span>
                                        </div>
                                    {% endif %}

                                    <button type="button" class="b-button b-button_green b-button_circle b-button_circle-plus js-count-inc {{= inc_disabled ? 'b-button_disabled' : '' }}" type="button"></button>
                                </span>
                                <span class="js-incart-default {{= product.inCartQuantity ? 'm-hidden' : ''}}">
                                    {{= app_format.readableQuantity(quantity, product.productType.code === 'WEIGHT')}}
                                </span>
                            </div>

                            <div class="b-product-line__col">
                                <div class="b-product-line__price b-price js-price">
                                    {{= product.inCartTotalPrice ? app_format.price(product.inCartTotalPrice.value) : app_format.price(product.unitPrice) }}
                                </div>

                                <div class="b-product-line__measure">
                                    <span class="js-unit-price">{{= app_format.price(product.unitPrice, false, false) }}</span>

                                    {% if (product.productType.code === 'WEIGHT'): %}
                                        {% if (product.showUnit === 'кг.'): %}
                                            за 1 {{= product.showUnit }}
                                        {% else: %}
                                            за {{= product.showUnit }}
                                        {% endif %}
                                    {% elseif (product.productType.code === 'PIECE_WEIGHT' || product.productType.code === 'PIECE'): %}
                                        {% if (product.standardWeight): %}
                                            за 1 шт ({{= product.standardWeight }} г)
                                        {% else: %}
                                            за 1 шт
                                        {% endif %}
                                    {% endif %}
                                </div>

                                <div class="b-product-line__old b-price js-price-old {{= product.oldPrice ? '' : 'm-hidden' }}">
                                    {% if (product.oldPrice): %}
                                        {{= app_format.price(product.oldPrice, false, false) }}
                                    {% endif %}
                                </div>
                            </div>

                            <div class="b-product-line__col">
                                <button class="b-product-line__add b-button b-button_green {{= product.inCartQuantity ? 'js-cart-button' : 'js-cart-add'}}" type="button" {{= product.inCartQuantity ? 'disabled' : '' }} onmousedown="try { rrApi.addToBasket({{= product.code }}) } catch(e) {}">
                                    {% if (product.inCartQuantity): %}
                                        <i></i>Добавлен
                                    {% else: %}
                                        <i></i>В корзину
                                    {% endif %}
                                </button>
                            </div>
                        </article>
                    {% endfor %}
                </div>

                <div class="b-order-repeat__actions">
                    <button class="b-order-repeat__actions-btn b-button b-button_white js-popular-to-cart" type="button">Перейти в корзину</button>
                    <button class="b-order-repeat__actions-btn b-button b-button_green js-close" type="button">Продолжить покупки</button>
                </div>
            </script>
        <script id="modal-time-left" type="text/template">
    <div class="b-time-left-modal">
        <span class="b-time-left-modal__progress js-time-left-progress"></span>
        {% if (stockType === 'PICKUP'): %}
            <span class="b-time-left-modal__img b-time-left-modal__img_pickup"></span>
            <span class="b-time-left-modal__text">Вы выбрали резервирование<a href="https://av.ru/personal/delivery_products/#alcohol" class="b-time-left-modal__delivery-rules-link" title="Общие правила доставки">*</a> алкоголя</span>
            <span class="b-time-left-modal__slogan">Продолжите резервирование алкоголя в Энотеке</span>
        {% else: %}
            <span class="b-time-left-modal__time">{{= timeLeft }}</span>
            <span class="b-time-left-modal__text" data-str="{{= minuteStr }}">{{= minuteStr }}</span>
            <span class="b-time-left-modal__slogan">Успейте оформить заказ</span>
        {% endif %}
    </div>
</script>

