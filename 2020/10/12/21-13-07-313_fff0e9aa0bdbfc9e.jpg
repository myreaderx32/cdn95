                {% for (var i in list): %}
                    <div class="b-search-multi__select-item js-multisearch-modal-select-item{{= list[i].id === select_id ? ' m-selected' : '' }}" data-id="{{= list[i].id }}">{{= list[i].name }}</div>
                {% endfor %}
            </div>
        </div>
    </div>
    <button class="b-button b-button_green b-search-multi__lists-add js-multisearch-modal-add" type="button">
        <span class="b-search-multi__lists-plus"></span>
